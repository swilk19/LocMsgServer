<h1>Add new Message</h1>
<%= form_for :message, url: messages_path do |f| %>
  <p>
    <%= f.label :lat %><br>
    <%= f.text_field :lat %>
  </p>
 
  <p>
    <%= f.label :long %><br>
    <%= f.text_field :long %>
  </p>

  <p>
    <%= f.label :msg %><br>
    <%= f.text_area :msg %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
