<h1>Add new Message</h1>
<%= form_for :message do |f| %>
  <p>
    <%= f.lat :lat %><br>
    <%= f.text_field :lat %>
  </p>
 
  <p>
    <%= f.long :long %><br>
    <%= f.text_field :long %>
  </p>

  <p>
    <%= f.msg :msg %><br>
    <%= f.text_area :msg %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
